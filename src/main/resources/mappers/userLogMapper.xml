<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 <mapper namespace="com.diary.userLog.dao.UserLogDAO">
 	<insert id="" parameterType="com.diary.userLog.model.UserLog">
 		INSERT INTO `user_log`
 		(
 			`userId`
			,`age`
			,`category`
			,`amount`
 		)
 		VALUES
 		(
 			#{userId}
 			,#{age}
 			,#{category}
 			,#{amount}
 		)
 	</insert>
 </mapper>